<h1>Listing quotes</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Ref</th>
      <th>Date</th>
      <th>Status</th>
      <th>Total vat</th>
      <th>Total price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotes.each do |quote| %>
      <tr>
        <td><%= quote.project_id %></td>
        <td><%= quote.ref %></td>
        <td><%= quote.date %></td>
        <td><%= quote.status %></td>
        <td><%= quote.total_vat %></td>
        <td><%= quote.total_price %></td>
        <td><%= link_to 'Show', quote %></td>
        <td><%= link_to 'Edit', edit_quote_path(quote) %></td>
        <td><%= link_to 'Destroy', quote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quote', new_quote_path %>
