$('td#'+<%= @expend.id %>+'_status').html('paid');

$('td#'+<%= @expend.id %>+'_links').children('a.line_reconcile_icon_hidden').attr('class', 'line_reconcile_icon_show');
<% unless @expend.transfer? %>
$('td#'+<%= @expend.id %>+'_links').children('a.line_edit_icon_hidden').attr('class', 'line_edit_icon_show');
<% end %>
$('td#'+<%= @expend.id %>+'_links').children('a.line_delete_icon_hidden').attr('class', 'line_delete_icon_show');

$('td#'+<%= @expend.id %>+'_links').children('a.line_unreconcile_icon_show').attr('class', 'line_unreconcile_icon_hidden');
