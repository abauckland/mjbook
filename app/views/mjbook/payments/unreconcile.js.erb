$('td#'+<%= @payment.id %>+'_status').html('confirmed');

$('td#'+<%= @payment.id %>+'_links').children('a.line_reconcile_icon_hidden').attr('class', 'line_reconcile_icon_show');

$('td#'+<%= @payment.id %>+'_links').children('a.line_unreconcile_icon_show').attr('class', 'line_unreconcile_icon_hidden');
<% if @payment.transfer? %>
    $('td#'+<%= @payment.id %>+'_links').children('a.line_delete_icon_hidden').attr('class', 'line_delete_icon_show');
<% end %>